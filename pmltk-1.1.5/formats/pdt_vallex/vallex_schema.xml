     <value>CONTRD</value>
     <value>COND</value>
     <value>CPHR</value>
     <value>CPR</value>
     <value>CRIT</value>
     <value>DIFF</value>
     <value>DIR1</value>
     <value>DIR2</value>
     <value>DIR3</value>
     <value>DPHR</value>
     <value>ETHD</value>
     <value>EXT</value>
     <value>HER</value>
