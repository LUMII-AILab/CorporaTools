<?xml version="1.0" encoding="utf-8"?>

<pml_schema xmlns="http://ufal.mff.cuni.cz/pdt/pml/schema/" version="1.1">
 <root name="valency_lexicon" type="valency_lexicon.type" />
 <type name="valency_lexicon.type">
  <structure>
   <member as_attribute="1" name="owner" required="1">
    <cdata format="PMLREF" />
   </member>
   <member name="body" type="body.type" />
   <member name="head" type="head.type" />
   <member name="tail" type="tail.type" />
  </structure>
 </type>
 <type name="head.type">
  <structure>
   <member name="list_of_users" type="list_of_users.type" />
   <member name="global_history" type="global_history.type" />
  </structure>
 </type>
 <type name="tail.type">
  <sequence content_pattern="(forbid+)">
   <element name="forbid" type="forbid.type" />
  </sequence>
 </type>
 <type name="forbid.type">
  <container>
   <attribute name="id" role="#ID" required="1">
    <cdata format="ID" />
   </attribute>
  </container>
 </type>
 <type name="body.type">
  <sequence content_pattern="(word+)">
   <element name="word" type="word.type" />
  </sequence>
 </type>
 <type name="global_history.type">
  <sequence content_pattern="(global_event*)">
   <element name="global_event" type="global_event.type" />
  </sequence>
 </type>
 <type name="global_event.type">
  <container>
   <attribute name="time_stamp" required="1">
    <cdata format="any" />
   </attribute>
   <attribute name="author" required="1">
    <cdata format="any" />
   </attribute>
   <cdata format="any" />
  </container>
 </type>
 <type name="list_of_users.type">
  <sequence content_pattern="(user+)">
   <element name="user" type="user.type" />
  </sequence>
 </type>
 <type name="user.type">
  <container>
   <attribute name="reviewer">
    <choice>
     <value>YES</value>
     <value>NO</value>
    </choice>
   </attribute>
   <attribute name="name" required="1">
    <cdata format="any" />
   </attribute>
   <attribute name="id" role="#ID" required="1">
    <cdata format="ID" />
   </attribute>
   <attribute name="annotator">
    <choice>
     <value>YES</value>
     <value>NO</value>
    </choice>
   </attribute>
  </container>
 </type>
 <type name="word.type">
  <structure>
   <member as_attribute="1" name="id" role="#ID" required="1">
    <cdata format="ID" />
   </member>
   <member as_attribute="1" name="lemma" required="1">
    <cdata format="any" />
   </member>
   <member as_attribute="1" name="POS">
    <choice>
     <value>V</value>
     <value>A</value>
     <value>N</value>
     <value>D</value>
    </choice>
   </member>
   <member name="valency_frames" type="valency_frames.type" />
   <member name="note" type="note.type" />
   <member name="problems" type="problems.type" />
  </structure>
 </type>
 <type name="problems.type">
  <sequence content_pattern="(problem+)">
   <element name="problem" type="problem.type" />
  </sequence>
 </type>
 <type name="problem.type">
  <container>
   <attribute name="solved">
    <choice>
     <value>YES</value>
     <value>NO</value>
    </choice>
   </attribute>
   <attribute name="author">
    <cdata format="PMLREF" />
   </attribute>
   <cdata format="any" />
  </container>
 </type>
 <type name="valency_frames.type">
  <sequence content_pattern="(frame*)">
   <element name="frame" type="frame.type" />
  </sequence>
 </type>
 <type name="frame.type">
  <structure>
   <member as_attribute="1" name="status">
    <choice>
     <value>active</value>
     <value>reviewed</value>
     <value>substituted</value>
     <value>obsolete</value>
     <value>deleted</value>
    </choice>
   </member>
   <member as_attribute="1" name="used">
    <cdata format="any" />
   </member>
   <member as_attribute="1" name="id" role="#ID" required="1">
    <cdata format="ID" />
   </member>
   <member as_attribute="1" name="hereditary_used">
    <cdata format="any" />
   </member>
   <member as_attribute="1" name="substituted_with">
    <cdata format="IDREFS" />
   </member>
   <member name="example" type="example.type" />
   <member name="frame_elements" type="frame_elements.type" />
   <member name="local_history" type="local_history.type" />
   <member name="problems" type="problems.type" />
   <member name="note" type="note.type" />
  </structure>
 </type>
 <type name="example.type">
  <cdata format="any" />
 </type>
 <type name="note.type">
  <cdata format="any" />
 </type>
 <type name="frame_elements.type">
  <sequence content_pattern="(element|element_alternation)*">
   <element name="element_alternation" type="element_alternation.type" />
   <element name="element" type="element.type" />
  </sequence>
 </type>
 <type name="element_alternation.type">
  <sequence content_pattern="(element+)">
   <element name="element" type="element.type" />
  </sequence>
 </type>
 <type name="element.type">
  <container>
   <attribute name="functor" required="1">
    <choice>
     <value>-</value>
