     <value>RESTR</value>
     <value>RSTR</value>
     <value>SUBS</value>
     <value>TFHL</value>
     <value>TFRWH</value>
     <value>THL</value>
     <value>THO</value>
     <value>TOWH</value>
     <value>TPAR</value>
     <value>TSIN</value>
     <value>TTILL</value>
     <value>TWHEN</value>
     <value>TOWH</value>
     <value>VOCAT</value>
    </choice>
   </attribute>
   <attribute name="orig_type">
    <cdata format="any" />
   </attribute>
   <attribute name="type">
    <choice>
     <value>oblig</value>
     <value>non-oblig</value>
    </choice>
   </attribute>
   <attribute name="forms">
    <cdata format="any" />
   </attribute>
   <sequence content_pattern="(form*)">
    <element name="form" type="form.type" />
   </sequence>
  </container>
 </type>
 <type name="form.type">
  <container>
   <attribute name="abbrev">
    <cdata format="any" />
   </attribute>
   <sequence content_pattern="(typical | elided | recip | state | (parent?, node*, (parentpos, node*)?))">
    <element name="parentpos" type="parentpos.type" />
    <element name="elided" type="elided.type" />
    <element name="parent" type="parent.type" />
    <element name="recip" type="recip.type" />
    <element name="typical" type="typical.type" />
    <element name="node" type="node.type" />
    <element name="state" type="state.type" />
   </sequence>
  </container>
 </type>
 <type name="typical.type">
  <container></container>
 </type>
 <type name="elided.type">
  <container></container>
 </type>
 <type name="recip.type">
  <container></container>
 </type>
 <type name="state.type">
  <container></container>
 </type>
 <type name="parent.type">
  <structure>
   <member name="node" type="node.type" />
